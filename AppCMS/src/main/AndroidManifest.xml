<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="snagfilms.com.air.appcms">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name="com.viewlift.AppCMSApplication"
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/default_app_name"
        android:roundIcon="@drawable/logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.viewlift.AppCMSLaunchActivity" android:launchMode="singleTask">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.viewlift.views.activity.AppCMSPageActivity"
            android:configChanges="orientation|screenSize"
            android:launchMode="singleTask"/>
        <activity android:name="com.viewlift.views.activity.AppCMSErrorActivity" />
        <activity android:name="com.viewlift.views.activity.AppCMSNavItemsActivity" />
        <activity android:name="com.viewlift.activities.AppCMSLaunchTestActivity"></activity>
        <activity android:name="com.viewlift.views.activity.AppCMSSearchActivity" />
    </application>

</manifest>