<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@drawable/home_screen_background"
    tools:context="com.viewlift.tv.views.activity.AppCmsMyProfileFragment">


    <FrameLayout
        android:id="@+id/sub_navigation_placholder"
        android:layout_width="match_parent"
        android:layout_height="@dimen/appcms_tv_sub_navigation_height"
        android:visibility="visible">
    </FrameLayout>


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/sub_navigation_placholder">

        <FrameLayout
            android:id="@+id/profile_placeholder"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
        >

        </FrameLayout>

    </RelativeLayout>

</LinearLayout>
